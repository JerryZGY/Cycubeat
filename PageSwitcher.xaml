<Window x:Class="Cycubeat.PageSwitcher"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cycubeat"
        mc:Ignorable="d"
        Title="Cycubeat" d:DesignWidth="1366" d:DesignHeight="768"
        Width="{Binding SystemParameters.PrimaryScreenHeight}" Height="{Binding SystemParameters.PrimaryScreenHeight}"
        ResizeMode="NoResize" WindowStyle="None" WindowState="Maximized" Background="Black">
    <Viewbox>
        <Grid>
            <ContentPresenter x:Name="Presenter"/>
            <Canvas Name="Canvas_Pointer">
                <Image Name="PointerFirst" Width="50" Height="50" Visibility="Collapsed" Source="Materials/PointerFirst.png"/>
                <Image Name="PointerSecond" Width="50" Height="50" Visibility="Collapsed" Source="Materials/PointerSecond.png"/>
            </Canvas>
        </Grid>
    </Viewbox>
</Window>